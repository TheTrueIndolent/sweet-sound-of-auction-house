							<Button name="$parentChoice4" inherits="ssoaPopOutButton"/>
						</Frames>
					</Button>
				</Frames>
			</Frame>
<!-- Second Options Box Expire Options -->
			<Frame name="$parentBox2" inherits="ssoaOptionsBox">
				<Anchors>
