							<Anchor point="BOTTOMRIGHT" relativeTo="$parentPopOut1" relativePoint="TOP" x="0" y="0"/>
						</Anchors>
						<Frames>
							<Button name="$parentChoice1" inherits="ssoaPopOutButton"/>
							<Button name="$parentChoice2" inherits="ssoaPopOutButton"/>
						</Frames>
					</Button>
<!-- Pop Out Button 3 eMote Notification -->
